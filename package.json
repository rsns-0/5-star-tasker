		"sapphire": "sapphire",
		"generate": "sapphire generate",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:watchui": "vitest --ui --api 9527",
		"doc": "typedoc",
		"doc:open": "docs/index.html",
		"localdoc": "typedoc --out .localdocs && ./localdocs/index.html",
		"bot": "yarn build && tsx src/index.ts",
		"dbpull": "prisma db pull && prisma generate",
		"dbpush": "prisma db push",
		"build": "swc src -d dist",
		"watch": "swc src -d dist -w",
		"start": "node dist/index.js",
		"dev": "run-s build start",
		"watch:start": "tsc-watch --onSuccess \"node ./dist/index.js\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"pgenerate": "prisma generate",
		"lint": "prettier --check \"src/**/*.ts\"",
		"prettify": "prettier --write \"src/**/*.ts\""
